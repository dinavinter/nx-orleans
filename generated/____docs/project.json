{
  "name": "____docs",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "outputs": ["{projectRoot}"]
    },
    "doc2": {
      "executor": "@schemaland/net-doc-plugin:docfx-build",
      "options": {
        "build": {
          "content": ["**/*.{md|yml}"],
          "resource": ["**/media/**"],
          "dest": "{projectRoot}/_site",
          "globalMetadata": {
            "_appTitle": "QuickStart Documentation",
            "_appName": "QUickStart Documentation",
            "_appLogoPath": "images/logo.png",
            "_appFaviconPath": "images/favicon.ico",
            "_enableSearch": true,
            "_enableNewTab": true
          }
        }
      }
    },
    "doc": {
      "executor": "@schemaland/net-doc-plugin:docfx",
      "options": {
        "output": "generated/docs/references/grain-controllers",
        "inputs": [],
        "config": "generated/docs/docfx.json"
      },
      "inputs": ["libs/grain-controllers"]
    }
  },
  "implicitDependencies": ["grains", "grain-controllers"]
}
