{
  "output": "generated/docs/_site",
  "build": {
    "content": [
      "references/**/*.{md|yml}"
    ],
    "resource": [
      "**/media/**"
    ],
    "globalMetadata": {
      "_appTitle": "NX + Net + Orleans + Docfx",
      "_appName": "NX + Net + Orleans + Docfx",
      "_appLogoPath": "images/logo.png",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true,
      "_enableNewTab": true
    }
  },
  "inputs": [
    "generated/docs/references/**/*"
  ],
  "config": "generated/docs/docfx.json"
}