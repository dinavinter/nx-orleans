{
  "name": "grain-controllers",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "@nx-dotnet/core:build",
      "outputs": [
        "{workspaceRoot}/dist/libs/grain-controllers",
        "{workspaceRoot}/dist/intermediates/libs/grain-controllers"
      ],
      "options": {
        "configuration": "Debug",
        "noDependencies": true
      },
      "configurations": {
        "production": {
          "configuration": "Release"
        }
      }
    },
    "lint": {
      "executor": "@nx-dotnet/core:format"
    },
    "docfx": {
      "executor": "@schemaland/net-doc-plugin:docfx"
    },
    "doc": {
      "executor": "@schemaland/net-doc-plugin:docfx",
      "options": {
        "output": "generated/docs/references/grain-controllers",
        "input": "libs/grain-controllers",
        "outputProject": "docs"
      }
    }
  },
  "projectType": "library"
}
